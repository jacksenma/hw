
toastr.options = {
		  "closeButton": false,
		  "debug": false,
		  "newestOnTop": false,
		  "progressBar": false,
		  "positionClass": "toast-top-center",
		  "preventDuplicates": false,
		  "onclick": null,
		  "showDuration": "300",
		  "hideDuration": "1000",
		  "timeOut": "1000",
		  "extendedTimeOut": "1000",
		  "showEasing": "swing",
		  "hideEasing": "linear",
		  "showMethod": "fadeIn",
		  "hideMethod": "fadeOut"
		}



function adminPay(){
	$.ajax({
        type: "POST",
        url: "adminPass",
        success: function(data){
     	   if(data=="ok"){
     		  toastr.success('操作成功');
     		  $(id).remove();
     		  showNothing();
     	   }
           }
     });
	toastr.success("OK");
}